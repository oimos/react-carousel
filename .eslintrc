{
  "extends": [
    "eslint-config-airbnb-base"
  ],
  "rules": {
    "func-names": 0,
    "object-shorthand": 0,
    "no-new": 0,
    "import/no-unresolved": 0,
    "no-unused-vars": 0,
    "max-len": 0,
    "prefer-template": 0
  },
  "globals": {
    "window": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}